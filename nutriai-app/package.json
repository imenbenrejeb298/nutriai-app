{
  "name": "nutriai-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "lucide-react": "^0.552.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "react-scripts": "5.0.1",
    "recharts": "^3.3.0",
    "web-vitals": "^2.1.4"
    ,
    "i18next": "^23.7.7",
    "react-i18next": "^13.0.0"
  },
  "main": "electron-main.js",
  "scripts": {
    "start": "react-scripts start",
  "build": "react-scripts build",
  "build:web": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev:server": "npm --prefix server start",
    "dev:all": "concurrently \"npm start\" \"npm --prefix server start\"",
    "cap:copy": "npx cap copy",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "electron:serve": "npm run build && electron .",
    "electron:build": "electron-builder",
    "dev:electron": "concurrently \"npm start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/ios": "^7.4.4",
  "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.0",
    "electron": "^25.0.0",
    "electron-builder": "^24.5.0",
  "postcss": "^8.5.6",
    "wait-on": "^7.0.1"
  },
  "proxy": "http://localhost:5000",
  "build": {
    "appId": "com.nutriai.app",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "public"
    }
  }
}
